<?xml version="1.0" encoding="utf-8" ?>

<!-- Applications installed as part of mobile internet of Orange. -->
<ApplicationBehaviors
  xmlns="http://whatheco.de/abc/workspaces/windows/ApplicationBehaviors"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://whatheco.de/abc/workspaces/windows ApplicationBehaviors.xsd"
  Guid="{943768A2-0E3A-4902-9DBB-F8B7BEDD21AB}">

  <!-- Card detector is a background application that helps to detect USB modems, mobile Internet SIM cards. -->
  <Process Name="CardDetector" CompanyName="France Telecom">
    <IgnoreWindows>
      <!-- Card detector background windows. -->
      <Window ClassName="Afx:10000000:0:00010005:00000000:00000000" />
      <Window ClassName="Afx:10000000:0" />
    </IgnoreWindows>
  </Process>

</ApplicationBehaviors>